      :before-upload=\"uploadBefore\"\n                  :http-request=\"weiXinPayMethod\">\n                  <img style=\"width: 100px;height: 100px\" v-if=\"form.weixinPay\" :src=\"form.weixinPay\" class=\"avatar1\"\n                       alt=\"\">\n                  <i v-else class=\"el-icon-plus avatar-uploader-icon1\"></i>\n                </el-upload>\n              </el-form-item>\n            </el-col>\n          </el-row>\n          <el-row :gutter=\"24\">\n            <el-form-item label=\"网站评论\">\n              <el-radio v-for=\"item in openDictList\" :key=\"item.id\" v-model=\"form.openComment\" :label=\"item.value\"\n                        border size=\"medium\">{{ item.label }}\n              </el-radio>\n            </el-form-item>\n          </el-row>\n          <el-row :gutter=\"24\">\n            <el-form-item label=\"网站打赏\">\n              <el-radio v-for=\"item in openDictList\" :key=\"item.id\" v-model=\"form.openAdmiration\" :label=\"item.value\"\n                        border size=\"medium\">{{ item.label }}\n              </el-radio>\n            </el-form-item>\n          </el-row>\n          <el-form-item>\n            <el-button v-if=\"canUpdate\" type=\"primary\" @click=\"submitForm()\">保 存</el-button>\n          </el-form-item>\n        </el-form>\n      </el-tab-pane>\n\n      <el-tab-pane>\n        <span slot=\"label\">\n          <i class=\"el-icon-date\"></i> 关注我们\n        </span>\n        <el-form\n          style=\"margin-left: 20px;\"\n          label-position=\"left\"\n          :model=\"form\"\n          label-width=\"80px\"\n          :rules=\"rules\"\n          ref=\"form\"\n        >\n          <el-checkbox-group v-model=\"showList\">\n            <el-form-item label=\"邮箱\" prop=\"email\">\n              <el-input v-model=\"form.email\" style=\"width: 400px\"></el-input>\n              <el-checkbox label=\"1\" style=\"margin-left: 10px\">在首页显示</el-checkbox>\n            </el-form-item>\n            <el-form-item label=\"QQ号\" prop=\"qqNumber\">\n              <el-input v-model=\"form.qqNumber\" style=\"width: 400px\"></el-input>\n              <el-checkbox label=\"2\" style=\"margin-left: 10px\">在首页显示</el-checkbox>\n            </el-form-item>\n            <el-form-item label=\"github\" prop=\"github\">\n              <el-input v-model=\"form.github\" style=\"width: 400px\"></el-input>\n              <el-checkbox label=\"3\" style=\"margin-left: 10px\">在首页显示</el-checkbox>\n            </el-form-item>\n            <el-form-item label=\"Gitee\" prop=\"gitee\">\n              <el-input v-model=\"form.gitee\" style=\"width: 400px\"></el-input>\n              <el-checkbox label=\"4\" style=\"margin-left: 10px\">在首页显示</el-checkbox>\n            </el-form-item>\n          </el-checkbox-group>\n          <el-form-item>\n            <el-button v-if=\"canUpdate\" type=\"primary\" @click=\"submitForm()\">保 存</el-button>\n          </el-form-item>\n        </el-form>\n      </el-tab-pane>\n    </el-tabs>\n  </div>\n</template>\n\n<script>\nimport {getWebConfig, update} from \"@/api/webConfig\";\nimport {getDataByDictType} from \"@/api/dictData\"\nimport {mapGetters} from \"vuex\";\nimport {hasAuth} from \"@/utils/auth\";\nimport {upload} from \"@/api/imgUpload\";\n\nexport default {\n  data() {\n    return {\n      loginTypeLists: [],\n      showList: [],\n      loading: [],\n      img: process.env.VUE_APP_IMG_API,\n      form: {\n        name: \"\",\n        aboutMe: \"\",\n        webUrl: \"\",\n        keyword: \"\",\n        summary: \"\",\n        author: \"\",\n        logo: \"\",\n        recordNum: \"\",\n        openComment: \"1\",\n        aliPay: \"\",\n        weixinPay: \"\",\n        authorAvatar: \"\",\n        authorInfo: \"\",\n        touristAvatar: \"\",\n        bulletin: \"\",\n        showList: \"\",\n        loginTypeList: \"\"\n      },\n      systemConfig: {},\n      uploadPictureHost: process.env.VUE_APP_BASE_API + \"/file/upload\",\n      openDictList: [], //字典\n     